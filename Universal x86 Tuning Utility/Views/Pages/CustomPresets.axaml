                        <Expander.Header>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <StackPanel Grid.Column="0" VerticalAlignment="Center">
                                    <TextBlock
                                        FontSize="14"
                                        FontWeight="Medium"
                                        Text="AMD Precision Boost Overdrive" />
                                    <TextBlock
                                        FontSize="12"
                                        Foreground="{DynamicResource TextFillColorTertiaryBrush}"
                                        Text="Provides the ability to customise PBO options." />
                                </StackPanel>
                            </Grid>
                        </Expander.Header>
                        <!--  CardExpander content  -->
                        <StackPanel>
                            <StackPanel Margin="0,-8,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontWeight="DemiBold" FontSize="13"
                                               Foreground="{DynamicResource TextFillColorPrimaryBrush}">
                                        PBO Scalar
                                    </TextBlock>
                                    <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}" Margin="6,0,0,-2"
                                                   Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                                   ToolTip.Tip="Allows control to change the PBO Scalar which adjusts the FIT/FITness/FailuresInTime limit by the set amount"
                                                   VerticalAlignment="Bottom" />
                                </StackPanel>

                                <Grid Margin="0,8,0,0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="33" />
                                        <ColumnDefinition Width="150" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <CheckBox Name="cbPBOScaler"
                                              IsChecked="{Binding SelectedPreset.IsPboScalar, UpdateSourceTrigger=PropertyChanged}"/>
                                    <ui:NumberBox Name="nudPBOScaler"
                                                  Value="{Binding SelectedPreset.PboScalar, UpdateSourceTrigger=PropertyChanged}" Maximum="10"
                                                  Minimum="1" Grid.Column="1" SmallChange="1"  />
                                    <Slider Name="sdPBOScaler" Value="{Binding SelectedPreset.PboScalar, UpdateSourceTrigger=PropertyChanged}"
                                            Maximum="10" Minimum="1" Grid.Column="2" Margin="12,0,0,0"
                                            TickFrequency="1" VerticalAlignment="Center" IsSnapToTickEnabled="True"
                                             />
                                </Grid>
                            </StackPanel>
                        </StackPanel>
                    </Expander>

                    <!-- Amd Curve optimizer settings !-->
                    <Expander Name="sdAmdCO"
                              IsEnabled="True"
                              IsExpanded="False"
                              Icon="TopSpeed20" Margin="0,8,15,0">
                        <Expander.Header>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <StackPanel Grid.Column="0" VerticalAlignment="Center">
                                    <TextBlock
                                        FontSize="14"
                                        FontWeight="Medium"
                                        Text="AMD Curve Optimiser" />
                                    <TextBlock
                                        FontSize="12"
                                        Foreground="{DynamicResource TextFillColorTertiaryBrush}"
                                        Text="Provides the ability to customise Curve Optimiser options." />
                                </StackPanel>
                            </Grid>
                        </Expander.Header>
                        <!--  CardExpander content  -->
                        <StackPanel>
                            <StackPanel Margin="0,-8,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontWeight="DemiBold" FontSize="13"
                                               Foreground="{DynamicResource TextFillColorPrimaryBrush}">
                                        All Core Offset
                                    </TextBlock>
                                    <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}" Margin="6,0,0,-2"
                                                   Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                                   ToolTip.Tip="Allows control to change the all core Curve Optimiser Frequency/Voltage curve offset"
                                                   VerticalAlignment="Bottom" />
                                </StackPanel>

                                <Grid Margin="0,8,0,0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="33" />
                                        <ColumnDefinition Width="150" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <CheckBox Name="cbAllCO"
                                              IsChecked="{Binding SelectedPreset.IsCoAllCore, UpdateSourceTrigger=PropertyChanged}"/>
                                    <ui:NumberBox Name="nudAllCO"
                                                  Value="{Binding SelectedPreset.CoAllCore, UpdateSourceTrigger=PropertyChanged}" Maximum="30"
                                                  Minimum="-50" Grid.Column="1" SmallChange="1"  />
                                    <Slider Name="sdAllCO" Value="{Binding SelectedPreset.CoAllCore, UpdateSourceTrigger=PropertyChanged}"
                                            Maximum="30" Minimum="-50" Grid.Column="2" Margin="12,0,0,0"
                                            TickFrequency="1" VerticalAlignment="Center" IsSnapToTickEnabled="True"
                                             />
                                </Grid>
                                <StackPanel>
                                    <StackPanel Orientation="Horizontal" Margin="0,8,0,0">
                                        <TextBlock FontWeight="DemiBold" FontSize="13"
                                                   Foreground="{DynamicResource TextFillColorPrimaryBrush}">
                                            iGPU Offset
                                        </TextBlock>
                                        <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}" Margin="6,0,0,-2"
                                                       Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                                       ToolTip.Tip="Allows control to change the iGPU Curve Optimiser Frequency/Voltage curve offset"
                                                       VerticalAlignment="Bottom" />
                                    </StackPanel>
                                    <Grid Margin="0,8,0,0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="33" />
                                            <ColumnDefinition Width="150" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <CheckBox Name="cbGfxCO" 
                                                  IsChecked="{Binding SelectedPreset.IsCoGfx, UpdateSourceTrigger=PropertyChanged}"/>
                                        <ui:NumberBox Name="nudGfxCO"
                                                      Value="{Binding SelectedPreset.CoGfx, UpdateSourceTrigger=PropertyChanged}" Maximum="30"
                                                      Minimum="-50" Grid.Column="1" SmallChange="1"
                                                       />
                                        <Slider Name="sdGfxCO" Value="{Binding SelectedPreset.CoGfx, UpdateSourceTrigger=PropertyChanged}"
                                                Maximum="30" Minimum="-50" Grid.Column="2" Margin="12,0,0,0"
                                                TickFrequency="1" VerticalAlignment="Center" IsSnapToTickEnabled="True"
                                                 />
                                    </Grid>
                                </StackPanel>
                            </StackPanel>
                        </StackPanel>
                    </Expander>

                    <!-- Amd CCD1 settings !-->
                    <Expander Name="sdAmdCCD1CO"
                              Visibility="Collapsed"
                              IsExpanded="False"
                              Margin="0,8,15,0">
                        <Expander.Header>
                            <StackPanel VerticalAlignment="Center">
                                <ui:SymbolIcon Symbol="{ic:FluentIcon TopSpeed20Regular}"/>
                                <TextBlock
                                    FontSize="14"
                                    FontWeight="Medium"
                                    Text="AMD Per-Core Curve Optimiser (CCD1)" />
                                <TextBlock
                                    FontSize="12"
                                    Foreground="{DynamicResource TextFillColorTertiaryBrush}"
                                    Text="Provides the ability to customise per-core Curve Optimiser options." />
                            </StackPanel>
                        </Expander.Header>
                        <!--  CardExpander content  -->
                        <ItemsControl ItemsSource="{Binding SelectedPreset.Ccd1States, UpdateSourceTrigger=PropertyChanged}">
                            <ItemsControl.ItemTemplate>
                                <DataTemplate DataType="{x:Type models:AmdCcdCoreState}">
                                    <StackPanel Margin="0,-8,0,0">
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock FontWeight="DemiBold"
                                                       FontSize="13"
                                                       Text="{Binding CoreIndex, StringFormat={}Core {0} Offset}"/>
                                            <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}" 
                                                           Margin="6,0,0,-2"
                                                           ToolTip.Tip="{Binding CoreIndex, StringFormat={}Allows control to change the Curve Optimiser Frequency/Voltage curve offset for core {1}}"
                                                           VerticalAlignment="Bottom" />
                                        </StackPanel>

                                        <Grid ColumnDefinitions="33, 150, *" 
                                              Margin="0,8,0,0">
                                            <CheckBox Grid.Column="0"
                                                      IsChecked="{Binding IsEnabled, UpdateSourceTrigger=PropertyChanged}"/>
                                            
                                            <ui:NumberBox Grid.Column="1" 
                                                          Value="{Binding Value, UpdateSourceTrigger=PropertyChanged}" Maximum="30"
                                                          Minimum="-50"
                                                          SmallChange="1"  />
                                            <Slider Grid.Column="2"
                                                    Margin="12,0,0,0"
                                                    Value="{Binding Value, UpdateSourceTrigger=PropertyChanged}"
                                                    Maximum="30" 
                                                    Minimum="-50" 
                                                    TickFrequency="1" 
                                                    VerticalAlignment="Center" 
                                                    IsSnapToTickEnabled="True"/>
                                        </Grid>
                                    </StackPanel>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                        </ItemsControl>
                    </Expander>

                    <!-- Amd CCD2 settings !-->
                    <Expander Name="sdAmdCCD2CO"
                              Visibility="Collapsed"
                              IsEnabled="True"
                              IsExpanded="False"
                              Margin="0,8,15,0">
                        <Expander.Header>
                            <StackPanel VerticalAlignment="Center">
                                <ui:SymbolIcon Symbol="{ic:FluentIcon TopSpeed20Regular}"/>
                                <TextBlock
                                    FontSize="14"
                                    FontWeight="Medium"
                                    Text="AMD Per-Core Curve Optimiser (CCD2)" />
                                <TextBlock
                                    FontSize="12"
                                    Foreground="{DynamicResource TextFillColorTertiaryBrush}"
                                    Text="Provides the ability to customise per-core Curve Optimiser options." />
                            </StackPanel>
                        </Expander.Header>
                        <!--  CardExpander content  -->
                        <ItemsControl ItemsSource="{Binding SelectedPreset.Ccd2States, UpdateSourceTrigger=PropertyChanged}">
                            <ItemsControl.ItemTemplate>
                                <DataTemplate DataType="{x:Type models:AmdCcdCoreState}">
                                    <StackPanel Margin="0,-8,0,0">
                                        <StackPanel Orientation="Horizontal">
                                            <TextBlock FontWeight="DemiBold"
                                                       FontSize="13"
                                                       Text="{Binding CoreIndex, StringFormat={}Core {0} Offset}"/>
                                            <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}" 
                                                           Margin="6,0,0,-2"
                                                           ToolTip.Tip="{Binding CoreIndex, StringFormat={}Allows control to change the Curve Optimiser Frequency/Voltage curve offset for core {1}}"
                                                           VerticalAlignment="Bottom" />
                                        </StackPanel>

                                        <Grid ColumnDefinitions="33, 150, *" 
                                              Margin="0,8,0,0">
                                            <CheckBox Grid.Column="0"
                                                      IsChecked="{Binding IsEnabled, UpdateSourceTrigger=PropertyChanged}"/>
                                            
                                            <ui:NumberBox Grid.Column="1" 
                                                          Value="{Binding Value, UpdateSourceTrigger=PropertyChanged}" Maximum="30"
                                                          Minimum="-50"
                                                          SmallChange="1"  />
                                            <Slider Grid.Column="2"
                                                    Margin="12,0,0,0"
                                                    Value="{Binding Value, UpdateSourceTrigger=PropertyChanged}"
                                                    Maximum="30" 
                                                    Minimum="-50" 
                                                    TickFrequency="1" 
                                                    VerticalAlignment="Center" 
                                                    IsSnapToTickEnabled="True"/>
                                        </Grid>
                                    </StackPanel>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                        </ItemsControl>
                    </Expander>

                    <!-- Amd power (boost) profile settings !-->
                    <ui:CardControl Name="sdAmdPowerProfile"
                                    IsEnabled="True"
                                    Icon="FastAcceleration20" Margin="0,8,15,0">
                        <ui:CardControl.Header>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <StackPanel Grid.Column="0" VerticalAlignment="Center">
                                    <TextBlock
                                        FontSize="14"
                                        FontWeight="Medium"
                                        Text="AMD Boost Profile" />
                                    <TextBlock
                                        FontSize="12"
                                        Foreground="{DynamicResource TextFillColorTertiaryBrush}"
                                        Text="Provides the ability to set a manual boost profile which impact on boost delay." />
                                </StackPanel>
                                <ComboBox
                                    Grid.Column="1"
                                    VerticalAlignment="Center"
                                    SelectedIndex="{Binding SelectedPreset.BoostProfile, UpdateSourceTrigger=PropertyChanged}"
                                    MinWidth="164" Name="cbxBoost"
                                    Height="37">
                                    <ComboBoxItem>Auto</ComboBoxItem>
                                    <ComboBoxItem>Power Saving</ComboBoxItem>
                                    <ComboBoxItem>Performance</ComboBoxItem>
                                </ComboBox>

                            </Grid>
                        </ui:CardControl.Header>
                    </ui:CardControl>

                    <!-- Display settings !-->
                    <ui:CardControl Name="sdRefreshRate"
                                    IsEnabled="True"
                                    Icon="Desktop20" Margin="0,8,15,0">
                        <ui:CardControl.Header>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <StackPanel Grid.Column="0" VerticalAlignment="Center">
                                    <TextBlock
                                        FontSize="14"
                                        FontWeight="Medium"
                                        Text="Display Refresh Rate" />
                                    <TextBlock
                                        FontSize="12"
                                        Foreground="{DynamicResource TextFillColorTertiaryBrush}"
                                        Text="Provides the ability to set a custom supported display refresh rate. May not work for everyone!" />
                                </StackPanel>
                                <ComboBox Name="cbxRefreshRate"
                                          Grid.Column="1"
                                          ItemsSource="{Binding RefreshRates, UpdateSourceTrigger=PropertyChanged}"
                                          SelectedValue="{Binding SelectedPreset.DisplayHz, UpdateSourceTrigger=PropertyChanged}"
                                          VerticalAlignment="Center" 
                                          SelectedIndex="0"
                                          MinWidth="164"
                                          Height="37" />

                            </Grid>
                        </ui:CardControl.Header>
                    </ui:CardControl>

                    <!-- Power mode settings !-->
                    <ui:CardControl Name="sdPowerMode"
                                    IsEnabled="True"
                                    Icon="Power20" Margin="0,8,15,0">
                        <ui:CardControl.Header>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <StackPanel Grid.Column="0" VerticalAlignment="Center">
                                    <TextBlock
                                        FontSize="14"
                                        FontWeight="Medium"
                                        Text="Windows Power Mode" />
                                    <TextBlock
                                        FontSize="12"
                                        Foreground="{DynamicResource TextFillColorTertiaryBrush}"
                                        Text="Provides the ability to change the Windows power mode from a preset." />
                                </StackPanel>
                                <ComboBox
                                    Grid.Column="1"
                                    VerticalAlignment="Center" 
                                    SelectedIndex="{Binding SelectedPreset.PowerMode, UpdateSourceTrigger=PropertyChanged}" 
                                    MinWidth="164" Name="cbxPowerMode"
                                    Height="37">
                                    <ComboBoxItem>
                                        System Controlled
                                    </ComboBoxItem>
                                    <ComboBoxItem>
                                        Best Power Effciency
                                    </ComboBoxItem>
                                    <ComboBoxItem>
                                        Balanced
                                    </ComboBoxItem>
                                    <ComboBoxItem>
                                        Best Performance
                                    </ComboBoxItem>
                                </ComboBox>

                            </Grid>
                        </ui:CardControl.Header>
                    </ui:CardControl>

                    <!-- Asus power settings !-->
                    <ui:CardControl Name="sdAsusPower"
                                    IsVisible="{Binding IsAsusPowerSettingsAvailable, UpdateSourceTrigger=PropertyChanged}"
                                    Icon="Power20" Margin="0,8,15,0">
                        <ui:CardControl.Header>
                            <Grid ColumnDefinitions="*, Auto">
                                <StackPanel Grid.Column="0" 
                                            VerticalAlignment="Center">
                                    <TextBlock
                                        FontSize="14"
                                        FontWeight="Medium"
                                        Text="ASUS Power Profile" />
                                    <TextBlock
                                        FontSize="12"
                                        Foreground="{DynamicResource TextFillColorTertiaryBrush}"
                                        Text="Provides the ability to set an ASUS power profile. This will override Armoury Crate!" />
                                </StackPanel>
                                <ComboBox Grid.Column="1"
                                          VerticalAlignment="Center"
                                          SelectedValue="{Binding SelectedAsusPowerProfile, UpdateSourceTrigger=PropertyChanged}"
                                          ItemsSource="{Binding AsusPowerProfiles, UpdateSourceTrigger=PropertyChanged}"
                                          MinWidth="164" 
                                          Height="37">
                                    <ComboBox.ItemTemplate>
                                        <DataTemplate DataType="{x:Type models:AsusPowerProfile}">
                                            <Label Content="{Binding Title}"/>
                                        </DataTemplate>
                                    </ComboBox.ItemTemplate>
                                </ComboBox>

                            </Grid>
                        </ui:CardControl.Header>
                    </ui:CardControl>

                    <!-- Asus gpu mode !-->
                    <ui:CardControl Name="sdAsusUlti"
                                    IsVisible="{Binding IsAsusGpuUltimateSettingsAvailable, UpdateSourceTrigger=PropertyChanged}"
                                    Icon="FastAcceleration20"
                                    Margin="0,8,15,0">
                        <ui:CardControl.Header>
                            <Grid ColumnDefinitions="*, Auto">
                                <StackPanel Grid.Column="0" 
                                            VerticalAlignment="Center">
                                    <TextBlock FontSize="14"
                                               FontWeight="Medium"
                                               Text="ASUS GPU Ultimate Mode" />
                                    <TextBlock FontSize="12"
                                               Foreground="{DynamicResource TextFillColorTertiaryBrush}"
                                               Text="Provides the ability to manually toggle MUX switch on/off. This will override Armoury Crate!" />
                                </StackPanel>

                                <StackPanel Orientation="Horizontal" 
                                            Grid.Column="1">
                                    <ToggleSwitch IsChecked="{Binding SelectedPreset.AsusGpuUlti, UpdateSourceTrigger=PropertyChanged}" 
                                                  Margin="0,0,9,0" />
                                </StackPanel>
                            </Grid>
                        </ui:CardControl.Header>
                    </ui:CardControl>

                    <!-- Asus eco mode settings !-->
                    <ui:CardControl Name="sdAsusEco"
                                    IsEnabled="True"
                                    Icon="LeafTwo20" Margin="0,8,15,0">
                        <ui:CardControl.Header>
                            <Grid ColumnDefinitions="*, Auto">
                                <StackPanel Grid.Column="0"
                                            VerticalAlignment="Center">
                                    <TextBlock FontSize="14"
                                               FontWeight="Medium"
                                               Text="ASUS iGPU Only Mode" />
                                    <TextBlock FontSize="12"
                                               Foreground="{DynamicResource TextFillColorTertiaryBrush}"
                                               Text="Provides the ability to toggle iGPU only mode on/off. This will override Armoury Crate!" />
                                </StackPanel>

                                <StackPanel Orientation="Horizontal" 
                                            Grid.Column="1">
                                    <ToggleSwitch IsChecked="{Binding SelectedPreset.AsusIGpu, UpdateSourceTrigger=PropertyChanged}"
                                                  Margin="0,0,9,0" />
                                </StackPanel>
                            </Grid>
                        </ui:CardControl.Header>
                    </ui:CardControl>
                </StackPanel>
            </ScrollViewer>
        </DockPanel>
    </Grid>
</UserControl>