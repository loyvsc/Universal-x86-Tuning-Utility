<UserControl x:Class="Universal_x86_Tuning_Utility.Views.Pages.CustomPresetsPage"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:ui="using:FluentAvalonia.UI.Controls"
             xmlns:ic="using:FluentAvalonia.FluentIcons"
             xmlns:viewmodels="clr-namespace:Universal_x86_Tuning_Utility.ViewModels"
             xmlns:models="clr-namespace:ApplicationCore.Models;assembly=ApplicationCore"
             mc:Ignorable="d"
             x:DataType="viewmodels:CustomPresetsViewModel"
             d:DesignHeight="2500"
             d:DesignWidth="800">
    <Grid>
        <DockPanel>
            <TextBlock FontSize="26"
                       FontWeight="Medium"
                       Text="Custom Presets"
                       DockPanel.Dock="Top"
                       Margin="18,10,0,0" />

            <DockPanel DockPanel.Dock="Top"
                       Margin="18,9,18,0">
                <StackPanel Orientation="Horizontal"
                            DockPanel.Dock="Left"
                            HorizontalAlignment="Left">
                    <TextBlock FontSize="17"
                               FontWeight="Medium"
                               Margin="0,0,12,0"
                               VerticalAlignment="Center"
                               Text="Saved Presets:" />
                    <ComboBox Width="225"
                              ItemsSource="{Binding AvailablePresets, UpdateSourceTrigger=PropertyChanged}"
                              Height="37" />
                </StackPanel>
                <StackPanel Orientation="Horizontal"
                            DockPanel.Dock="Right"
                            HorizontalAlignment="Right">
                    <TextBlock FontSize="17"
                               FontWeight="Medium"
                               Margin="0,0,12,0"
                               VerticalAlignment="Center"
                               Text="New Preset Name:" />
                    <TextBox Name="tbxPresetName"
                             Margin="0,0,-2,0"
                             Width="225"
                             Height="35" 
                             Text="{Binding SelectedPreset.Name, UpdateSourceTrigger=PropertyChanged}"/>
                </StackPanel>
            </DockPanel>

            <StackPanel Orientation="Horizontal"
                        DockPanel.Dock="Bottom"
                        HorizontalAlignment="Right"
                        Margin="0,9,9,6">
                <Button HorizontalAlignment="Right"
                        VerticalAlignment="Center"
                        Margin="0,0,9,9"
                        Height="40"
                        DockPanel.Dock="Bottom"
                        Width="50"
                        IsVisible="{Binding UndoActionAvailable, UpdateSourceTrigger=PropertyChanged}"
                        ToolTip.Tip="Undo overclock settings"
                        Command="{Binding UndoCommand}">
                    <ui:SymbolIcon Symbol="Sync" FontSize="24" />
                </Button>
                <Button HorizontalAlignment="Right"
                        VerticalAlignment="Center"
                        Margin="0,0,9,9"
                        Height="40"
                        DockPanel.Dock="Bottom"
                        Width="50"
                        ToolTip.Tip="Reload settings of current preset values"
                        Command="{Binding ReloadPresetValuesCommand}">
                    <ui:SymbolIcon Symbol="Sync" FontSize="24" />
                </Button>
                <Button HorizontalAlignment="Right"
                        VerticalAlignment="Center"
                        Margin="0,0,9,9"
                        Height="40"
                        DockPanel.Dock="Bottom"
                        Width="50"
                        ToolTip.Tip="Delete current settings preset"
                        Command="{Binding DeletePresetCommand}">
                    <ui:SymbolIcon Symbol="Delete" FontSize="24" />
                </Button>
                <Button HorizontalAlignment="Right"
                        VerticalAlignment="Center"
                        Margin="0,0,9,9"
                        Height="40"
                        DockPanel.Dock="Bottom"
                        Width="50"
                        ToolTip.Tip="Save current settings as new preset"
                        Command="{Binding SavePresetCommand}">
                    <ui:SymbolIcon Symbol="SaveFilled" FontSize="24" />
                </Button>
                <Button HorizontalAlignment="Right"
                        VerticalAlignment="Center"
                        Margin="0,0,9,9"
                        Height="40"
                        Width="50"
                        DockPanel.Dock="Bottom"
                        ToolTip.Tip="Apply current settings"
                        Command="{Binding ApplyPresetCommand}">
                    <ui:SymbolIcon Symbol="Document" FontSize="24" />
                </Button>
            </StackPanel>

            <ScrollViewer Margin="0,9,0,0"
                          HorizontalScrollBarVisibility="Auto"
                          VerticalScrollBarVisibility="Auto"
                          IsDeferredScrollingEnabled="False">
                <StackPanel Margin="15,0,0,0">
                    <!-- Amd Apu Thermal settings !-->
                    <Expander Margin="0,0,15,0"
                              IsVisible="{Binding is}">
                        <Expander.Header>
                            <StackPanel Grid.Column="0"
                                        VerticalAlignment="Center">
                                <ui:SymbolIcon Symbol="{ic:FluentIcon Temperature20Regular}" />
                                <TextBlock
                                    FontSize="14"
                                    FontWeight="Medium"
                                    Text="APU Temperature Tuning" />
                                <TextBlock
                                    FontSize="12"
                                    Foreground="{DynamicResource TextFillColorTertiaryBrush}"
                                    Text="Provides the ability to set APU thermal targets." />
                            </StackPanel>
                        </Expander.Header>
                        <!--  CardExpander content  -->
                        <StackPanel>
                            <StackPanel Margin="0,-8,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontWeight="DemiBold"
                                               FontSize="13"
                                               Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                               Text="Temperature Limit (°C)" />
                                    <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}"
                                                   Margin="6,0,0,-2"
                                                   Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                                   ToolTip.Tip="Controls the temperature limit at which the APU starts soft throttling"
                                                   VerticalAlignment="Bottom" />
                                </StackPanel>

                                <Grid ColumnDefinitions="33, 150, *"
                                      Margin="0,8,0,0">
                                    <CheckBox Grid.Column="0"
                                              IsChecked="{Binding SelectedPreset.IsApuTemp, UpdateSourceTrigger=PropertyChanged}"/>
                                    <ui:NumberBox Grid.Column="1"
                                                  Value="{Binding SelectedPreset.ApuTemperature, UpdateSourceTrigger=PropertyChanged}"
                                                  Maximum="105"
                                                  Minimum="10"
                                                  SmallChange="1" />
                                    <Slider Grid.Column="2"
                                            Value="{Binding SelectedPreset.ApuTemperature, UpdateSourceTrigger=PropertyChanged}"
                                            Maximum="105"
                                            Minimum="10"
                                            TickFrequency="1"
                                            VerticalAlignment="Center"
                                            IsSnapToTickEnabled="True" />
                                </Grid>
                            </StackPanel>
                            <StackPanel Margin="0,8,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontWeight="DemiBold"
                                               FontSize="13"
                                               Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                               Text="Skin Temperature Limit (°C)" />
                                    <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}"
                                                   Margin="6,0,0,-2"
                                                   Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                                   ToolTip.Tip="Controls the laptop chassis temperature limit at which the APU starts throttling"
                                                   VerticalAlignment="Bottom" />
                                </StackPanel>
                                <Grid ColumnDefinitions="33, 150, *"
                                      Margin="0,8,0,0">
                                    <CheckBox Name="cbAPUSkinTemp"
                                              IsChecked="{Binding SelectedPreset.IsApuSkinTemp, UpdateSourceTrigger=PropertyChanged}"/>
                                    <ui:NumberBox Grid.Column="1"
                                                  Value="{Binding SelectedPreset.ApuSkinTemperature, UpdateSourceTrigger=PropertyChanged}"
                                                  Maximum="105"
                                                  Minimum="8"
                                                  SmallChange="1" />
                                    <Slider Grid.Column="2"
                                            Value="{Binding SelectedPreset.ApuSkinTemperature, UpdateSourceTrigger=PropertyChanged}"
                                            Maximum="105"
                                            Minimum="8"
                                            Margin="12,0,0,0"
                                            TickFrequency="1"
                                            VerticalAlignment="Center"
                                            IsSnapToTickEnabled="True" />
                                </Grid>
                            </StackPanel>
                        </StackPanel>
                    </Expander>

                    <!-- Amd cpu thermal settings !-->
                    <Expander Name="sdAmdCpuThermal"
                              Margin="0,8,15,0"
                              IsExpanded="False">
                        <Expander.Header>
                            <StackPanel Grid.Column="0" VerticalAlignment="Center">
                                <ui:SymbolIcon Symbol="{ic:FluentIcon Temperature20Regular}" />
                                <TextBlock
                                    FontSize="14"
                                    FontWeight="Medium"
                                    Text="CPU Temperature Tuning" />
                                <TextBlock
                                    FontSize="12"
                                    Foreground="{DynamicResource TextFillColorTertiaryBrush}"
                                    Text="Provides the ability to set CPU thermal targets." />
                            </StackPanel>
                        </Expander.Header>
                        <!--  CardExpander content  -->
                        <StackPanel>
                            <StackPanel Margin="0,-8,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontWeight="DemiBold" FontSize="13"
                                               Foreground="{DynamicResource TextFillColorPrimaryBrush}">
                                        Temperature Limit (°C)
                                    </TextBlock>
                                    <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}" 
                                                   Margin="6,0,0,-2"
                                                   Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                                   ToolTip.Tip="Controls the temperature limit at which the CPU starts hard throttling"
                                                   VerticalAlignment="Bottom" />
                                </StackPanel>

                                <Grid ColumnDefinitions="33, 150, *" Margin="0,8,0,0">
                                    <CheckBox Name="cbCPUTemp" />
                                    <ui:NumberBox Name="nudCPUTemp"
                                                  Grid.Column="1"
                                                  Text="{Binding ElementName=sdCPUTemp, Path=Value}"
                                                  Value="{Binding ElementName=sdCPUTemp, Path=Value}"
                                                  Maximum="95"
                                                  Minimum="10"
                                                  SmallChange="1" />
                                    <Slider Name="sdCPUTemp"
                                            Grid.Column="2"
                                            Value="{Binding ElementName=nudCPUTemp, Path=Value}"
                                            Maximum="95"
                                            Minimum="10"
                                            Margin="12,0,0,0"
                                            TickFrequency="1"
                                            VerticalAlignment="Center"
                                            IsSnapToTickEnabled="True" />
                                </Grid>
                            </StackPanel>

                        </StackPanel>
                    </Expander>

                    <!-- Amd apu cpu settings !-->
                    <Expander Margin="0,8,15,0"
                              IsVisible="{Binding IsAmdApuSettingsAvailable, UpdateSourceTrigger=PropertyChanged}">
                        <Expander.Header>
                            <StackPanel Grid.Column="0" VerticalAlignment="Center">
                                <ui:SymbolIcon Symbol="{ic:FluentIcon Power20Regular}" />
                                <TextBlock FontSize="14"
                                           FontWeight="Medium"
                                           Text="APU Power Tuning" />
                                <TextBlock FontSize="12"
                                           Foreground="{DynamicResource TextFillColorTertiaryBrush}"
                                           Text="Provides the ability to set APU power targets." />
                            </StackPanel>
                        </Expander.Header>
                        <!--  CardExpander content  -->
                        <StackPanel>
                            <StackPanel Margin="0,-8,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontWeight="DemiBold"
                                               FontSize="13"
                                               Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                               Text="STAPM Power Limit (W)" />
                                    <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}"
                                                   Margin="6,0,0,-2"
                                                   Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                                   ToolTip.Tip="Controls the APU's Skin Temperature Aware Power Management power limit"
                                                   VerticalAlignment="Bottom" />
                                </StackPanel>
                                <Grid ColumnDefinitions="33, 150, *"
                                      Margin="0,8,0,0">
                                    <CheckBox Grid.Column="0"
                                              Name="cbSTAPMPow"
                                              IsChecked="{Binding SelectedPreset.IsApuStapmPow, UpdateSourceTrigger=PropertyChanged}"/>
                                    <ui:NumberBox Name="nudSTAPMPow"
                                                  Grid.Column="1"
                                                  Value="{Binding SelectedPreset.ApuStapmPower, UpdateSourceTrigger=PropertyChanged}"
                                                  Maximum="300"
                                                  Minimum="5"
                                                  SmallChange="1" />
                                    <Slider Name="sdSTAPMPow"
                                            Grid.Column="2"
                                            Value="{Binding SelectedPreset.ApuStapmPower, UpdateSourceTrigger=PropertyChanged}"
                                            Maximum="300"
                                            Minimum="5"
                                            Margin="12,0,0,0"
                                            TickFrequency="1"
                                            VerticalAlignment="Center"
                                            IsSnapToTickEnabled="True" />
                                </Grid>
                            </StackPanel>
                            <StackPanel Margin="0,8,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontWeight="DemiBold"
                                               FontSize="13"
                                               Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                               Text="Slow Power Limit (W)" />
                                    <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}"
                                                   Margin="6,0,0,-2"
                                                   Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                                   ToolTip.Tip="Controls the APU's slow boost duration power limit. Within SmartShift enabled systems, this options controls the total power shared between the AMD CPU and AMD dGPU"
                                                   VerticalAlignment="Bottom" />
                                </StackPanel>
                                <Grid ColumnDefinitions="33, 150, *" Margin="0,8,0,0">
                                    <CheckBox Name="cbSlowPow"
                                              IsChecked="{Binding SelectedPreset.IsApuSlowPow, UpdateSourceTrigger=PropertyChanged}"/>
                                    <ui:NumberBox Name="nudSlowPow"
                                                  Grid.Column="1"
                                                  Value="{Binding SelectedPreset.ApuSlowPower, UpdateSourceTrigger=PropertyChanged}"
                                                  Maximum="300"
                                                  Minimum="5"
                                                  SmallChange="1" />
                                    <Slider Name="sdSlowPow"
                                            Grid.Column="2"
                                            Value="{Binding SelectedPreset.ApuSlowPower, UpdateSourceTrigger=PropertyChanged}"
                                            Maximum="300"
                                            Minimum="5"
                                            Margin="12,0,0,0"
                                            TickFrequency="1"
                                            VerticalAlignment="Center"
                                            IsSnapToTickEnabled="True" />
                                </Grid>
                            </StackPanel>
                            <StackPanel Margin="0,8,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontWeight="DemiBold" FontSize="13"
                                               Foreground="{DynamicResource TextFillColorPrimaryBrush}">
                                        Slow Boost Duration (s)
                                    </TextBlock>
                                    <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}" Margin="6,0,0,-2"
                                                   Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                                   ToolTip.Tip="Controls how long the APU stays within the slow boost power limit"
                                                   VerticalAlignment="Bottom" />
                                </StackPanel>
                                <Grid Margin="0,8,0,0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="33" />
                                        <ColumnDefinition Width="150" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <CheckBox Name="cbSlowTime"
                                              IsChecked="{Binding SelectedPreset.IsApuSlowTime, UpdateSourceTrigger=PropertyChanged}"/>
                                    <ui:NumberBox Name="nudSlowTime"
                                                  Value="{Binding SelectedPreset.ApuSlowTime, UpdateSourceTrigger=PropertyChanged}" Maximum="1024"
                                                  Minimum="2" Grid.Column="1" SmallChange="1" />
                                    <Slider Name="sdSlowTime" 
                                            Value="{Binding SelectedPreset.ApuSlowTime, UpdateSourceTrigger=PropertyChanged}"
                                            Maximum="1024"
                                            Minimum="2" Grid.Column="2" Margin="12,0,0,0"
                                            TickFrequency="1" VerticalAlignment="Center" IsSnapToTickEnabled="True" />
                                </Grid>
                            </StackPanel>

                            <StackPanel Margin="0,8,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontWeight="DemiBold" FontSize="13"
                                               Foreground="{DynamicResource TextFillColorPrimaryBrush}">
                                        Fast Power Limit (W)
                                    </TextBlock>
                                    <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}" Margin="6,0,0,-2"
                                                   Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                                   ToolTip.Tip="Controls the APU's fast boost duration power limit. Within SmartShift enabled systems, this options controls the total power shared between the AMD CPU and AMD dGPU"
                                                   VerticalAlignment="Bottom" />
                                </StackPanel>
                                <Grid Margin="0,8,0,0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="33" />
                                        <ColumnDefinition Width="150" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <CheckBox Name="cbFastPow" 
                                              IsChecked="{Binding SelectedPreset.IsApuFastPow, UpdateSourceTrigger=PropertyChanged}"/>
                                    <ui:NumberBox Name="nudFastPow"
                                                  Value="{Binding SelectedPreset.ApuFastPower, UpdateSourceTrigger=PropertyChanged}" 
                                                  Maximum="300"
                                                  Minimum="5" Grid.Column="1" SmallChange="1" />
                                    <Slider Name="sdFastPow" 
                                            Value="{Binding SelectedPreset.ApuFastPower, UpdateSourceTrigger=PropertyChanged}"
                                            Maximum="300" Minimum="5" Grid.Column="2" Margin="12,0,0,0"
                                            TickFrequency="1" VerticalAlignment="Center" IsSnapToTickEnabled="True" />
                                </Grid>
                            </StackPanel>
                            <StackPanel Margin="0,8,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontWeight="DemiBold" FontSize="13"
                                               Foreground="{DynamicResource TextFillColorPrimaryBrush}">
                                        Fast Boost Duration (s)
                                    </TextBlock>
                                    <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}" Margin="6,0,0,-2"
                                                   Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                                   ToolTip.Tip="Controls how long the APU stays within the fast boost power limit"
                                                   VerticalAlignment="Bottom" />
                                </StackPanel>
                                <Grid Margin="0,8,0,0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="33" />
                                        <ColumnDefinition Width="150" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <CheckBox Grid.Column="0" 
                                              Name="cbFastTime"
                                              IsChecked="{Binding SelectedPreset.IsApuStapmTime, UpdateSourceTrigger=PropertyChanged}"/>
                                    <ui:NumberBox Name="nudFastTime"
                                                  Value="{Binding SelectedPreset.ApuStapmTime, UpdateSourceTrigger=PropertyChanged}" 
                                                  Maximum="1024"
                                                  Minimum="2" Grid.Column="1" SmallChange="1" />
                                    <Slider Name="sdFastTime" Value="{Binding SelectedPreset.ApuStapmTime, UpdateSourceTrigger=PropertyChanged}"
                                            Maximum="1024" Minimum="2" Grid.Column="2" Margin="12,0,0,0"
                                            TickFrequency="1" VerticalAlignment="Center" IsSnapToTickEnabled="True"/>
                                </Grid>
                            </StackPanel>
                        </StackPanel>
                    </Expander>

                    <!-- Amd cpu settings !-->
                    <Expander
                        Margin="0,8,15,0"
                        IsEnabled="True"
                        IsExpanded="False"
                        Name="sdAmdCPU">
                        <Expander.Header>
                            <StackPanel VerticalAlignment="Center">
                                <ui:SymbolIcon Symbol="{ic:FluentIcon Power20Regular}"/>
                                <TextBlock
                                    FontSize="14"
                                    FontWeight="Medium"
                                    Text="CPU Power Tuning" />
                                <TextBlock
                                    FontSize="12"
                                    Foreground="{DynamicResource TextFillColorTertiaryBrush}"
                                    Text="Provides the ability to set CPU power targets." />
                            </StackPanel>
                        </Expander.Header>
                        <!--  CardExpander content  -->
                        <StackPanel>
                            <StackPanel Margin="0,-8,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontWeight="DemiBold" FontSize="13"
                                               Foreground="{DynamicResource TextFillColorPrimaryBrush}">
                                        PPT Limit (W)
                                    </TextBlock>
                                    <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}" Margin="6,0,0,-2"
                                                   Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                                   ToolTip.Tip="Controls the CPU's Package Power Tracking power limit"
                                                   VerticalAlignment="Bottom" />
                                </StackPanel>
                                <Grid Margin="0,8,0,0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="33" />
                                        <ColumnDefinition Width="150" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <CheckBox Name="cbPPT" />
                                    <ui:NumberBox Name="nudPPT" Text="{Binding ElementName=sdPPT, Path=Value}"
                                                  Value="{Binding ElementName=sdPPT, Path=Value}" Maximum="300"
                                                  Minimum="8" Grid.Column="1" SmallChange="1"  />
                                    <Slider Name="sdPPT" Value="{Binding ElementName=nudPPT, Path=Value}" Maximum="300"
                                            Minimum="8" Grid.Column="2" Margin="12,0,0,0" TickFrequency="1"
                                            VerticalAlignment="Center" IsSnapToTickEnabled="True"
                                             />
                                </Grid>
                            </StackPanel>
                            <StackPanel Margin="0,8,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontWeight="DemiBold" FontSize="13"
                                               Foreground="{DynamicResource TextFillColorPrimaryBrush}">
                                        EDC Limit (A)
                                    </TextBlock>
                                    <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}" Margin="6,0,0,-2"
                                                   Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                                   ToolTip.Tip="Controls the CPU's Electrical Design Current limit"
                                                   VerticalAlignment="Bottom" />
                                </StackPanel>
                                <Grid Margin="0,8,0,0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="33" />
                                        <ColumnDefinition Width="150" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <CheckBox Name="cbEDC" />
                                    <ui:NumberBox Name="nudEDC" Text="{Binding ElementName=sdEDC, Path=Value}"
                                                  Value="{Binding ElementName=sdEDC, Path=Value}" Maximum="900"
                                                  Minimum="8" Grid.Column="1" SmallChange="25"  />
                                    <Slider Name="sdEDC" Value="{Binding ElementName=nudEDC, Path=Value}" Maximum="900"
                                            Minimum="8" Grid.Column="2" Margin="12,0,0,0" TickFrequency="25"
                                            VerticalAlignment="Center" IsSnapToTickEnabled="True"
                                             />
                                </Grid>
                            </StackPanel>
                            <StackPanel Margin="0,8,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontWeight="DemiBold" FontSize="13"
                                               Foreground="{DynamicResource TextFillColorPrimaryBrush}">
                                        TDC Limit (A)
                                    </TextBlock>
                                    <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}" Margin="6,0,0,-2"
                                                   Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                                   ToolTip.Tip="Controls the CPU's Thermal Design Current limit"
                                                   VerticalAlignment="Bottom" />
                                </StackPanel>
                                <Grid Margin="0,8,0,0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="33" />
                                        <ColumnDefinition Width="150" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <CheckBox Name="cbTDC" />
                                    <ui:NumberBox Name="nudTDC" Text="{Binding ElementName=sdTDC, Path=Value}"
                                                  Value="{Binding ElementName=sdTDC, Path=Value}" Maximum="900"
                                                  Minimum="8" Grid.Column="1" SmallChange="25"  />
                                    <Slider Name="sdTDC" Value="{Binding ElementName=nudTDC, Path=Value}" Maximum="900"
                                            Minimum="8" Grid.Column="2" Margin="12,0,0,0" TickFrequency="25"
                                            VerticalAlignment="Center" IsSnapToTickEnabled="True"
                                             />
                                </Grid>
                            </StackPanel>
                        </StackPanel>
                    </Expander>

                    <!-- Intel cpu settings !-->
                    <Expander Margin="0,8,15,0"
                              Name="sdIntelCPU">
                        <Expander.Header>
                            <StackPanel VerticalAlignment="Center">
                                <ui:SymbolIcon Symbol="{ic:FluentIcon Power20Regular}"/>
                                <TextBlock
                                    FontSize="14"
                                    FontWeight="Medium"
                                    Text="CPU Power Tuning" />
                                <TextBlock
                                    FontSize="12"
                                    Foreground="{DynamicResource TextFillColorTertiaryBrush}"
                                    Text="Provides the ability to set CPU power targets." />
                            </StackPanel>
                        </Expander.Header>
                        <!--  CardExpander content  -->
                        <StackPanel>
                            <StackPanel Margin="0,-8,0,0">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontWeight="DemiBold" FontSize="13"
                                               Foreground="{DynamicResource TextFillColorPrimaryBrush}">
                                        Power Limit (W)
                                    </TextBlock>
                                    <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}" Margin="6,0,0,-2"
                                                   Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                                   ToolTip.Tip="Controls the CPU's total power limit"
                                                   VerticalAlignment="Bottom" />
                                </StackPanel>
                                <Grid Margin="0,8,0,0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="33" />
                                        <ColumnDefinition Width="150" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <CheckBox Name="cbIntelPL1" />
                                    <ui:NumberBox Name="nudIntelPL1"
                                                  Text="{Binding ElementName=sdIntelPL1, Path=Value}"
                                                  Value="{Binding ElementName=sdIntelPL1, Path=Value}" Maximum="400"
                                                  Minimum="8" Grid.Column="1" SmallChange="1"  />
                                    <Slider Name="sdIntelPL1" Value="{Binding ElementName=nudIntelPL1, Path=Value}"
                                            Maximum="400" Minimum="8" Grid.Column="2" Margin="12,0,0,0"
                                            TickFrequency="1" VerticalAlignment="Center" IsSnapToTickEnabled="True"
                                             />
                                </Grid>
                            </StackPanel>
                            <StackPanel Margin="0,8,0,0" 
                                        Visibility="Collapsed">
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock FontWeight="DemiBold" FontSize="13"
                                               Foreground="{DynamicResource TextFillColorPrimaryBrush}">
                                        Power Limit 2 (W)
                                    </TextBlock>
                                    <ui:SymbolIcon Symbol="{ic:FluentIcon Info20Regular}" Margin="6,0,0,-2"
                                                   Foreground="{DynamicResource TextFillColorPrimaryBrush}"
                                                   ToolTip.Tip="Controls the CPU's short boost power limit"
                                                   VerticalAlignment="Bottom" />
                                </StackPanel>
                                <Grid Margin="0,8,0,0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="33" />
                                        <ColumnDefinition Width="150" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>
                                    <CheckBox Name="cbIntelPL2" />
                                    <ui:NumberBox Name="nudIntelPL2"
                                                  Text="{Binding ElementName=sdIntelPL2, Path=Value}"
                                                  Value="{Binding ElementName=sdIntelPL2, Path=Value}" Maximum="400"
